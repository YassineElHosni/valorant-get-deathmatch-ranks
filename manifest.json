{
  "name": "Valorant",
  "description": "Get the latest rank of public users",
  "version": "0.0.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "/js/background.js"
  },
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["https://tracker.gg/valorant/*"],
  "action": {
    "default_icon": {
      "16": "/images/valorant16.png",
      "32": "/images/valorant32.png",
      "48": "/images/valorant48.png",
      "128": "/images/valorant128.png"
    }
  },
  "icons": {
    "16": "/images/valorant16.png",
    "32": "/images/valorant32.png",
    "48": "/images/valorant48.png",
    "128": "/images/valorant128.png"
  }
}